<script>
    // Use jQuery.noConflict() to release the '$' variable
    jQuery.noConflict();
    // Use jQuery instead of $ to ensure compatibility
    jQuery(function ($) {
        $("#draggable").draggable({
            snap: "#droppable", // Snap to the droppable area
            snapMode: "inner"   // Snap to the inside of the droppable area

        });
        $("#droppable").droppable({
            drop: function (event, ui) {
                $(this)
                    .addClass("ui-state-highlight")
                    .find("p")
                    .html("Dropped!");
            }
        });
        $("#droppable").droppable({
            drop: function (event, ui) {
                $(this)
                    .addClass("ui-state-highlight")
                    .find("p")
                    .html("Dropped!");
            }
        });
    });
</script>

